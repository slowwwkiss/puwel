  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const textToCopy = "ca coming 5hMvGNpwS7WdaFzhy9ZTUCbACMnjfJd8Bh9smn39pump"; // Replace with your actual CA

      function handleCopy(e) {
        e.preventDefault();
        const tempInput = document.createElement("input");
        tempInput.value = textToCopy;
        document.body.appendChild(tempInput);
        tempInput.select();
        document.execCommand("copy");
        tempInput.remove();
        alert("Copied: " + textToCopy);
      }

      const mobileBtn = document.getElementById("copy-mobile");
      const desktopBtn = document.getElementById("copy-desktop");

      if (mobileBtn) mobileBtn.addEventListener("click", handleCopy);
      if (desktopBtn) desktopBtn.addEventListener("click", handleCopy);
    });
  </script>
